@model Netotik.ViewModels.Shop.ProductPanel.ProductGridModel

<!-- Start page content -->
<div class="page-content">
    <!-- Start shop area -->
    <div class="shop-area" style="margin-top: 30px;">
        <div class="container">
            <div class="row">

                <div class="col-xs-12 col-sm-8 col-md-9">
                    @Html.Partial(MVC.Product.Views._List, Model)

                </div>



                <div class="col-xs-12 col-sm-4 col-md-3">
                    @Html.Action(MVC.Product.ActionNames.Filters,MVC.Product.Name)
                </div>
            </div>
        </div>
    </div>
    <!-- End shop area -->
</div>