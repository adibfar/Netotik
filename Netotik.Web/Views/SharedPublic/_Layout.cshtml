    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/Images/Logo/faveicon/favicon-16x16.png">
    <link rel="manifest" href="~/Content/Images/Logo/faveicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="~/Content/Images/Logo/faveicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">


    <!-- mobile settings -->
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

    <!-- CORE CSS -->
    <link href="~/Content/PublicUI/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <!-- THEME CSS -->
    <link href="~/Content/PublicUI/css/essentials.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PublicUI/css/layout.css" rel="stylesheet" type="text/css" />

    <!-- PAGE LEVEL SCRIPTS -->
    <link href="~/Content/PublicUI/css/header-1.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PublicUI/css/color_scheme/blue.css" rel="stylesheet" type="text/css" id="color_scheme" />

    <!-- REVOLUTION SLIDER -->
    <link href="~/Content/PublicUI/plugins/slider.revolution/css/extralayers.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PublicUI/plugins/slider.revolution/css/settings.css" rel="stylesheet" type="text/css" />


    <link href="~/Content/PublicUI/css/header-5.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PublicUI/css/layout-shop.css" rel="stylesheet" type="text/css" />



    <link href="~/Content/PublicUI/plugins/bootstrap/RTL/bootstrap-rtl.min.css" rel="stylesheet" type="text/css" id="rtl_ltr_b1" />
    <link href="~/Content/PublicUI/plugins/bootstrap/RTL/bootstrap-flipped.min.css" rel="stylesheet" type="text/css" id="rtl_ltr_b2" />
    <link href="~/Content/PublicUI/css/layout-RTL.css" rel="stylesheet" type="text/css" id="rtl_ltr" />
    
    <link href="~/Content/PublicUI/css/_layout-font-rewrite.farsi.css" rel="stylesheet" />

    @Html.Action(MVC.MetaTag.ActionNames.Index, MVC.MetaTag.Name, new { title = ViewBag.Title, keywords = ViewBag.Keywords, description = ViewBag.Description })
    @RenderSection("metatags", false)
    @Styles.Render("~/Content/publiccss")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("Top", false)

    <noscript>
        <meta http-equiv="refresh" content="0;url=/NoScript.html">
    </noscript>
</head>
<body class="smoothscroll enable-animation">
    <!--[if lt IE 8]>
            <p class="browserupgrade">شما از مرورگر<strong>قدیمی</strong> استفاده می کنید. لطفا<a href="http://browsehappy.com/">مرورگر خود را بروز رسانی کنید</a> برای نمایش هرچه بهتر سایت.</p>
        <![endif]-->
    <!-- Start main area -->

    

    <div id="wrapper">
        
        @Html.Action(MVC.Home.Header())
        @Html.Action(MVC.Home.HeaderMenu())

        @RenderBody()

        @Html.Action(MVC.Home.ActionNames.Footer, MVC.Home.Name)

    </div>


    <!-- SCROLL TO TOP -->
    <a href="#" id="toTop"></a>



    <!-- JAVASCRIPT FILES -->
    <script type="text/javascript">var plugin_path = '/Content/PublicUI/plugins/';</script>
    <script type="text/javascript" src="~/Content/PublicUI/plugins/jquery/jquery-2.2.3.min.js"></script>

    @*<script type="text/javascript" src="~/Content/PublicUI/js/scripts.js"></script>*@

    <!-- REVOLUTION SLIDER -->
    <script type="text/javascript" src="~/Content/PublicUI/plugins/slider.revolution/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="~/Content/PublicUI/plugins/slider.revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="~/Content/PublicUI/js/view/demo.revolution_slider.js"></script>
    <script type="text/javascript" src="~/Content/PublicUI/plugins/effects/canvas.particles.js"></script>


    @Scripts.Render("~/bundles/publicjs")

    @RenderSection("Down", required: false)
</body>
