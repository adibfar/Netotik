<!DOCTYPE html>
<html lang="fa">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">

    @Html.Action(MVC.MetaTag.Index())

    <link rel="apple-touch-icon" sizes="60x60" href="~/Content/ico/apple-icon-60.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/Content/ico/apple-icon-76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/Content/ico/apple-icon-120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/Content/ico/apple-icon-152.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/ico/favicon.ico">
    <link rel="shortcut icon" type="image/png" href="~/Content/ico/favicon-32.png">

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">

    <!-- BEGIN VENDOR CSS-->
    <link rel="stylesheet" href="~/Content/css/vendors.min.css" />
    <!-- BEGIN VENDOR CSS-->
    <!-- BEGIN Font icons-->
    <link rel="stylesheet" type="text/css" href="~/Content/fonts/icomoon.css">
    <link rel="stylesheet" type="text/css" href="~/Content/fonts/flag-icon-css/css/flag-icon.min.css">
    <!-- END Font icons-->
    <!-- BEGIN Plugins CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/sliders/slick/slick.css">
    <!-- END Plugins CSS-->
    <!-- BEGIN Vendor CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/forms/icheck/icheck.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/forms/icheck/custom.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/extensions/unslider.css">

    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/forms/spinner/jquery.bootstrap-touchspin.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/forms/toggle/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/forms/toggle/switchery.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/css/plugins/tables/datatable/dataTables.bootstrap4.min.css">
    <!-- END Vendor CSS-->
    <!-- BEGIN ROBUST CSS-->
    <link rel="stylesheet" href="~/Content/css/app.min.css" />
    <!-- END ROBUST CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/css/components/weather-icons/climacons.min.css">
    <!-- END Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/css/my-style.css">

    @RenderSection("ScriptUp", false)
</head>


<body data-open="hover" data-menu="horizontal-top-icon-menu" data-col="2-columns" class="horizontal-layout horizontal-top-icon-menu 2-columns ">

    @*<div id="preloader-wrapper">
            <div id="loader">
                <div class="line-scale loader-white">
                    <div></div>
                    <div></div>
                    <div></div>
                    <div></div>
                    <div></div>
                </div>
            </div>
            <div class="loader-section section-top bg-blue-grey"></div>
            <div class="loader-section section-bottom bg-blue-grey"></div>
        </div>*@

    @Html.Action(MVC.Home.Head())

    <div class="fullscreen-search-overlay"></div>

    @if (User.Identity.IsAuthenticated)
    {
        @Html.Action(MVC.Home.AdminMenu())
    }

    <div class="robust-content content container-fluid">
        <div class="content-wrapper">
            <div class="content-header">
                @Html.Partial(MVC.Shared.Views._BreadCrumb)
            </div>

            <div class="content-body">
                @Html.Partial(MVC.Shared.Views._Message)
                @RenderBody()
            </div>
        </div>
    </div>



    <footer class="footer footer-dark navbar-fixed-bottom">
        <p class="clearfix text-muted text-sm-center mb-0 px-2">
            <span class="float-md-right d-xs-block d-md-inline-block">
                Copyright  &copy; 2017 <a href="https://Dnma.ir" target="_blank" class="text-bold-800 grey darken-2">DNMA Groups </a>, All rights reserved
            </span><span class="float-md-left d-xs-block d-md-inline-block">
                Made With <i class="blue icon-windows2"></i> Microsoft Technology
            </span>
        </p>
    </footer>

    <!-- BEGIN VENDOR JS-->
    <script src="~/Content/js/vendors.min.js"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <script type="text/javascript" src="~/Content/js/plugins/ui/jquery.sticky.js"></script>
    
    <script src="~/Content/js/plugins/forms/spinner/jquery.bootstrap-touchspin.js" type="text/javascript"></script>
    <script src="~/Content/js/plugins/forms/validation/jqBootstrapValidation.js" type="text/javascript"></script>
    <script src="~/Content/js/plugins/forms/toggle/bootstrap-switch.min.js" type="text/javascript"></script>
    <script src="~/Content/js/plugins/forms/toggle/switchery.min.js" type="text/javascript"></script>

    <script src="~/Content/js/plugins/forms/icheck/icheck.min.js" type="text/javascript"></script>
    <script src="~/Content/js/components/forms/checkbox-radio.js" type="text/javascript"></script>

    <script src="~/Content/js/components/extensions/block-ui.js" type="text/javascript"></script>

    <script src="~/Content/js/plugins/tables/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="~/Content/js/plugins/tables/datatable/dataTables.bootstrap4.min.js" type="text/javascript"></script>
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN ROBUST JS-->
    <script src="~/Content/js/app.min.js"></script>
    <!-- END ROBUST JS-->
    <script type="text/javascript" src="~/Content/js/components/tooltip/tooltip.js"></script>


    @*@Scripts.Render("~/bundles/adminjs")*@
    
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("DownScript", false)


</body>
</html>