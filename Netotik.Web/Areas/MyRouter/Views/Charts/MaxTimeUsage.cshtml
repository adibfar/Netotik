        $(window).on("load", function () {
            CardBlock('#Section30Days');
            CardBlock('#Section365Days');
            CardBlock('#Section30DaysSessions');

            $.get("@Url.Action(MVC.MyRouter.Charts.GetMaxTimeUsage())", function (data) {
                var a = $("#month"),
                    b = {
                        tooltips: { mode: "label" },
                        responsive: !0,
                        maintainAspectRatio: !1,
                        responsiveAnimationDuration: 500,
                        scales: {
                            xAxes: [{stacked: !0, display: !0, gridLines:{ color: "#f3f3f3", drawTicks: !1 }, scaleLabel: { display: !0 }}],
                            yAxes: [{stacked: !0, display: !0, gridLines: { color: "#f3f3f3", drawTicks: !1 }, scaleLabel: { display: !0 }}]
                        }
                    },
                    c = {
                        labels: data.user,
                        datasets: [{
                            label: "@Captions.Time", data: data.Time,
                            backgroundColor: "#00BFA5", hoverBackgroundColor: "rgba(0,191,165,.8)", borderColor: "transparent"
                        }]
                    },
                    d = {
                        type: "bar", options: b, data: c };
                new Chart(a, d)
                CardUnBlock('#Section30Days');

                var a = $("#month"),
                    b = {
