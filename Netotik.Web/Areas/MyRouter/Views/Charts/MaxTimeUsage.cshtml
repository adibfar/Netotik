@{
    ViewBag.Title = Netotik.Resources.Captions.Charts;
    Layout = "~/Views/Shared/_Layout.cshtml";
    String temp = "";
}

<div class="col-xl-12">
    <div class="card Section30Days">
        <div class="card-body">
            <div class="card-block sales-growth-chart">
                <canvas id="month" height="300"></canvas>
            </div>
        </div>
        <div class="card-footer">
            <div class="chart-title mb-1 text-xs-center">
                <span class="text-muted">@Netotik.Resources.Captions.ChartOf30LastDays</span>
            </div>
        </div>
    </div>
</div>


<div class="col-xl-12">
    <div class="card">
        <div class="card-body">
            <div class="card-block sales-growth-chart">
                <canvas id="year" height="300"></canvas>
            </div>
        </div>
        <div class="card-footer">
            <div class="chart-title mb-1 text-xs-center">
                <span class="text-muted">@Netotik.Resources.Captions.ChartOf365LastDays</span>
            </div>
        </div>
    </div>
</div>

<div class="col-xl-12">
    <div class="card">
        <div class="card-body">
            <div class="card-block sales-growth-chart">
                <canvas id="lastsessions" height="300"></canvas>
            </div>
        </div>
