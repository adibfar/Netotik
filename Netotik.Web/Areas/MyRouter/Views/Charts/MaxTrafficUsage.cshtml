                    maintainAspectRatio: !1,
                    responsiveAnimationDuration: 500,
                    legend: {
                        position: "top"
                    },
                    scales: {
                        xAxes: [{
                            display: !0,
                            gridLines: {
                                color: "#f3f3f3",
                                drawTicks: !1
