                c = {
                    labels: [
                         @foreach(var item in ViewBag.UserListDownloadBySessions)
                         {
                             @Html.Raw('"')
                            @(item.Key.ToString())
                             @Html.Raw('"')
                             @(" , ")
                        }
                    ],
                    datasets: [{
                        label: "@Netotik.Resources.Captions.Download MB",
                        data: [
                            @foreach(var item in ViewBag.UserListDownloadBySessions)
                            {
                                temp = item.Value.ToString() + " , ";
