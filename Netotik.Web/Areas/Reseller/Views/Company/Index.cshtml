@using MvcPaging
@model IPagedList<Netotik.ViewModels.Identity.UserCompany.TableCompanyModel>
@{
    ViewBag.Title = "مدیریت کاربر ها";
}
<div class="row">
    <div class="col-lg-12" id="ContentPage">
        <section class="panel">
            <header class="panel-heading">
                @ViewBag.Title

                @Html.ActionLink("کاربر جدید", MVC.Reseller.Company.Create(), new { @class = "btn btn-primary pull-left" })

            </header>


            @Html.Partial("_Table", Model)

        </section>
    </div>
</div>
@section ScriptUp {
    <link href="~/Content/css/plugins/footable/footable.core.css" rel="stylesheet">
}
@section DownScript {
    <script src="~/Content/js/plugins/footable/footable.all.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.footable').footable();
        });
    </script>
}